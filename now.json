{
  "version": 2,
  "builds": [
    {
      "src": "nuxt.config.js",
      "use": "@nuxtjs/vercel-builder"
    }
  ],
  "env": {
    "API_URL": "http://localhost:4000/api",
    "ON_VERCEL": "true"
  },
  "build": {
    "env": {
      "API_URL": "http://localhost:4000/api"
    }
  },
  "routes": [{ "src": "/(.*)", "dest": "$1" }]
}
